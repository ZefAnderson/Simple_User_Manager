html 
    head 
        title= Vanguard
    body
        h1 Vanguard Contacts
        table 
            tr 
                th Name 
                th 
                th Email 
                th 
                th Username 
                th 
                th Age 
            each user in users
                tr 
                    td #{user.name}
                    td 
                    td #{user.email}
                    td 
                    td #{user.username}
                    td 
                    td #{user.age}
                    td 
                    include buttons.pug
        div 
            form(action="/") 
                input(type='submit' value='Add User')